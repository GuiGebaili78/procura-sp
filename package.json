{
  "name": "procura-sp",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "start http://localhost:3000 && docker-compose up -d",
    "dev:only": "next dev --turbopack",
    "dev:db": "docker-compose up -d && echo '⏳ Aguardando banco subir...' && timeout /t 10 /nobreak > nul",
    "build": "next build",
    "vercel-build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "generate:structure": "npx ts-node scripts/gerar_estrutura.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:restart": "docker-compose down && docker-compose up -d",
    "docker:reset": "docker-compose down -v && docker system prune -a --volumes -f && docker-compose build --no-cache && docker-compose up -d && echo '⏳ Aguardando banco e app subirem...' && timeout /t 30 /nobreak && echo '🚀 Executando migrações...' && curl http://localhost:3000/api/migrate && docker-compose up -d",
    "migrate": "curl http://localhost:3000/api/migrate",
    "migrate:prod": "npx ts-node -e \"import('./src/lib/migrations.ts').then(m => m.runMigrations())\"",
    "test": "jest --testPathPattern=unit --verbose",
    "test:integration": "jest --testPathPattern=integration --verbose",
    "test:all": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.1",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "next": "15.4.5",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.20",
    "@types/leaflet-routing-machine": "^3.2.9",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "^5"
  }
}
